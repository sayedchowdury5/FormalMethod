class Bank
        instance variables
		accounts : seq of Account;
		id : int;

        operations
			public getAccountById : int ==> Account
			getAccountById (id) == 
			(
				return id;
			);

			public getAccountByIdPin : int * int ==> Account
			getAccountByIdPin (id, pin) == 
			(
			    accounts . id := id;
				accounts . pin := pin;
				
			);
end Bank

